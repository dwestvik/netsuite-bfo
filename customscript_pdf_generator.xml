      <displaywidth>40</displaywidth>
      <dynamicdefault></dynamicdefault>
      <fieldtype>TEXT</fieldtype>
      <help></help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <label>PDF Template File</label>
      <linktext></linktext>
      <maxlength></maxlength>
      <maxvalue></maxvalue>
